<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive data from cloud backup -->
        <exclude domain="sharedpref" path="salsabil_prefs.xml"/>
        <exclude domain="database" path="salsabil_database"/>
    </cloud-backup>

    <device-transfer>
        <!-- Allow transfer of non-sensitive data -->
        <include domain="file" path="."/>
    </device-transfer>
</data-extraction-rules>